{{>header}}
<main class="main-page main-form">
    <section class="main-page-form">
        <h2>Usuario</h2>
        <form class="main-page-form-cadastro-usuario">
            <input type="text" name="nome" id="nome" placeholder="Nome usuario" required>

            <input type="email" name="email" id="email" placeholder="Email usuario" required>

            <input type="tel" name="telefone" id="telefone" placeholder="Telefone" required>

            <button type="submit">Cadastrar</button>
        </form>
    </section>
</main>